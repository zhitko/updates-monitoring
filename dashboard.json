            "query": "from(bucket: \"${Bucket}\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"updates\")\n  |> last()\n  |> limit(n:1)\n  |> group(columns: [\"contaier_id\"])\n  |> sort(columns: [\"contaier_id\", \"instance_name\"], desc: true)\n  |> map(fn: (r) => ({ r with \n      is_current_update: if r.local_current_digest != \"-\" and r.remote_current_digest != \"-\" and r.local_current_digest != r.remote_current_digest then \"rgba(39, 245, 41, 0.23)\" else \"transparent\",\n      is_latest_update: if r.local_current_digest != \"-\" and r.remote_latest_digest != \"-\" and r.local_current_digest != r.remote_latest_digest then \"rgba(39, 245, 41, 0.23)\" else \"transparent\",\n    }))",
